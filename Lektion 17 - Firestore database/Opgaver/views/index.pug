script.
    async function deletE(url, id) {
        const respons = await fetch(url + '/' + id, {
            method: "DELETE",
            headers: {
                'Content-Type': 'application/json'
            }
        });
        if (respons.status !== 201) // Deleted
            throw new Error(respons.status);
        return await respons.text();
    }

