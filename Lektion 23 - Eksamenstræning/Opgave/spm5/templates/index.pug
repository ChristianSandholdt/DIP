html
    head
        title Express-app 3
        script(src='index.js')
    body
        h1 Express app 
        input(type='text' placeholder ='Nummer' id="input")
        table 
            tr 
                th Navn 
                th Nummer 
            tbody(id='tbody')
                each kontakt in users 
                    tr 
                        td #{kontakt.navn}
                        td 
                            a(id="link" href="http://localhost:8080" onclick=`changeNumber(${kontakt.telefonnr})`) #{kontakt.telefonnr}
